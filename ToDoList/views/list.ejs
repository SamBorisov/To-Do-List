
<%- include('header') -%>
 
    <div class="box" id="heading">
        <h2> <%= listTitle %> </h2>
    </div>
    
    <div class="box">
      
            <% for (let i=0; i<newNotes.length; i++) { %>
            <div class="item">
                <input type="checkbox">
                <p> <%= newNotes[i] %> </p>
            </div>
            <% } %>
            
  
            <form class="item" action="/" method="post">
                <input type="text" placeholder="Note" name="note" placeholder="New Item" autocomplete="off"> 
                <button id="button1" class="btn btn-primary btn-lg" type="submit" name="list" value=<%= listTitle %>>+</button>
            </form>

    </div>



<%- include('footer') -%>